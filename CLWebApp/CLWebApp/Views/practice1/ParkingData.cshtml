@model CLWebApp.Models.ParkingInfo
@{
    ViewData["Title"] = "ParkingData";
}

<h2>ParkingData</h2>

<form asp-action="ParkingData" method="get">
    <div class="form-group">
        <label asp-for="ParkingName" class="control-label"></label>
        <select asp-items="ViewBag.Parking" asp-for="ParkingName" class="form-control input-sm" style="width: 300px;">
            <option value="">指定なし</option>
        </select>
        <span asp-validation-for="ParkingName" class="text-danger"></span>
    </div>

    <div class="form-group col-md-offset-5">
        <button type="submit" class="btn btn-warning">クリア</button>
    </div>
</form>

<div class="col-md-offset-5">
    <button type="submit" class="btn btn-primary">実行</button>
</div>
